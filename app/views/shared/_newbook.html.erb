<h2>New book</h2>
  <%= form_with model: book do |f| %>
    <div>
      <label>Title</label>
    </div>
    <%= f.text_field :title %>
    <div>
      <label>Body</label>
    </div>
    <%= f.text_area :body %>
    <br><%= f.submit 'Create Book', style: "color: #FFFFFF; background-color: #28a745; border: none; border-radius: 3px;",class: "btn btn-secondary"%>
  <% end %>